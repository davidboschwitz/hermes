<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.companyname.GMapTesting">
    <uses-sdk android:minSdkVersion="21" android:targetSdkVersion="27" />

    <!-- Google Maps for Android v2 requires OpenGL ES v2 -->
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />

    <!-- Necessary for apps that target Android 9.0 or higher -->
    <uses-library android:name="org.apache.http.legacy" android:required="false" />
  
    <!-- Permission to receive remote notifications from Google Play Services -->
    <!-- Notice here that we have the package name of our application as a prefix on the permissions. -->
    <uses-permission android:name="com.companyname.GMapTesting.permission.MAPS_RECEIVE" />
    <permission android:name="com.companyname.GMapTesting.permission.MAPS_RECEIVE" android:protectionLevel="signature" />

    <!-- These are optional, but recommended. They will allow Maps to use the My Location provider. -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    
    <application android:label="GMapTesting.Android">
      <meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="AIzaSyB7-JsPHYwPn0wm5d8kD3iSK-FRhJiFrfM" />
    
      <activity android:name=".BasicDemoActivity"> </activity>
      <activity android:name=".ErrorDialogFragment"> </activity>
      <activity android:name=".FeatureRowHolder"> </activity>
      <activity android:name=".GoogleMapHelpers"> </activity>
      <activity android:name=".MainActivity"> </activity>
      <activity android:name=".MapWithMarkersActivity"> </activity>
      <activity android:name=".MapWithOverlays"> </activity>
      <activity android:name=".MyLocationActivity"> </activity>
      <activity android:name=".SimpleActivityMetaData"> </activity>
      <activity android:name=".SamplesListAdapter"> </activity>
    
  
    </application>
  
</manifest>
